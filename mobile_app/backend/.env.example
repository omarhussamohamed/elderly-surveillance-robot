# Database Configuration
DATABASE_URL=postgresql://username:password@your-database-host:5432/database_name

# JWT Configuration
SECRET_KEY=your-super-secret-key-change-me
ALGORITHM=HS256

# AWS IoT Core Configuration
AWS_IOT_ENDPOINT=your-endpoint.iot.region.amazonaws.com
AWS_IOT_CLIENT_ID=backend-fastapi

# Certificate Paths (relative to backend directory)
AWS_ROOT_CA=certs/AmazonRootCA1.pem
AWS_CERT_FILE=certs/backend.cert.pem
AWS_PRIVATE_KEY=certs/backend.private.key
