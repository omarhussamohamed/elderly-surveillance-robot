<launch>

  <!-- ===================== -->
  <!-- KVS Streamer -->
  <!-- ===================== -->
  <node pkg="elderly_bot"
        type="kvs_streamer_node.py"
        name="kvs_streamer_node"
        output="screen">

    <!-- Load AWS / cloud configuration -->
    <rosparam file="$(find elderly_bot)/config/cloud/cloud_config.yaml"/>

    <!-- Stream parameters -->
    <param name="stream_name" value="RobotStream"/>
    <param name="width" value="1280"/>
    <param name="height" value="720"/>
    <param name="fps" value="15"/>
    <param name="bitrate" value="2000"/>

  </node>

</launch>
